CREATE TABLE EMPLOYEE(EMP_ID INT,
                      EMP_Name VARCHAR(20),
                      Department VARCHAR(20),
                      Salary INT,
                      PRIMARY KEY(EMP_ID));

CREATE TABLE EMPLOYEE(
    EMP_ID INT,
    EMP_Name VARCHAR(20),
    Department VARCHAR(20),
    Salary INT,
    PRIMARY KEY(EMP_ID)
);

INSERT ALL
      INTO EMPLOYEE VALUES(1,'David Smith','HR',3000)
      INTO EMPLOYEE VALUES(2,'Amit Kumar','Programming',9000)
      INTO EMPLOYEE VALUES(3,'Olivia Brown','Marketing',5000)
      INTO EMPLOYEE VALUES(4,'Chris Martin','Production',6000)
      INTO EMPLOYEE VALUES(5,'Arjun Patel','Sales',7500)
SELECT * FROM DUAL;

SELECT * FROM EMPLOYEE;

SELECT * FROM EMPLOYEE WHERE Salary>7000;

UPDATE EMPLOYEE SET Salary=8200 WHERE EMP_Name='David Smith';
SELECT * FROM EMPLOYEE;

INSERT INTO EMPLOYEE VALUES('6','Danniel Harris','Production',6700);
SELECT * FROM EMPLOYEE;

DELETE FROM EMPLOYEE WHERE EMP_ID=3;
SELECT * FROM EMPLOYEE;

SELECT * FROM EMPLOYEE WHERE UPPER(EMP_Name) NOT LIKE 'D%' AND UPPER(EMP_Name) NOT LIKE 'O%';

SELECT Department , SUM(Salary) AS Total_Salary FROM EMPLOYEE GROUP BY Department;

UPDATE EMPLOYEE SET SALARY=SALARY+(SALARY*0.05) WHERE Department = 'Production';
SELECT * FROM EMPLOYEE;

DELETE FROM EMPLOYEE WHERE Salary<7000;
SELECT * FROM EMPLOYEE;

SELECT EMP_Name, Salary FROM EMPLOYEE WHERE salary = ( SELECT MIN(Salary) FROM EMPLOYEE);
INSERT INTO EMPLOYEE
VALUES (7, 'Adith Jhon', 'Marketing', 6000);

UPDATE EMPLOYEE SET SALARY=8200 WHERE Department = 'Marketing';
SELECT * FROM EMPLOYEE;

SELECT EMP_Name FROM EMPLOYEE WHERE UPPER(EMP_Name) LIKE 'A%';

SELECT * FROM EMPLOYEE WHERE UPPER(EMP_Name) LIKE '%IT%';

SELECT UPPER(EMP_Name) AS EMP_NAME FROM EMPLOYEE ;

INSERT INTO EMPLOYEE VALUES (8, 'Rohit Kumar', 'Marketing', 8900);

SELECT * FROM EMPLOYEE WHERE UPPER(Department) LIKE 'M_R%' AND  UPPER(Department) LIKE '%KET%' ;

SELECT UPPER(DEPARTMENT) , REVERSE(UPPER(Department)) FROM EMPLOYEE ;



     
     
      

